# DO NOT EDIT THIS FILE!
#
# It's auto-generated by sonata-project/dev-kit package.

includes:
{% if file_exists(project_dir ~ '/phpstan-baseline.neon') %}
    - phpstan-baseline.neon
{% endif %}
    - vendor/phpstan/phpstan/conf/bleedingEdge.neon

parameters:
    level: 8
{% if file_exists(project_dir ~ '/phpstan-console-application.php') %}
    symfony:
        consoleApplicationLoader: ./phpstan-console-application.php
{% endif %}
{% if has_dependency(project_dir, 'phpstan/phpstan-doctrine') %}
    doctrine:
        allowNullablePropertyForRequiredField: true
{% endif %}
    paths:
        - src
        - tests
    scanFiles:
        - vendor/symfony/dependency-injection/Loader/Configurator/ContainerConfigurator.php
{% if file_exists(project_dir ~ '/src/Resources/skeleton') %}
    excludePaths:
        analyseAndScan:
            - src/Resources/skeleton/**
{% endif %}
    treatPhpDocTypesAsCertain: false
    checkInternalClassCaseSensitivity: true
    checkMissingVarTagTypehint: true
    checkMissingTypehints: true
    checkUninitializedProperties: true
{% if has_dependency(project_dir, 'phpstan/phpstan-symfony') %}
    featureToggles:
        skipCheckGenericClasses:
            - Symfony\Component\Form\AbstractType
            - Symfony\Component\Form\FormBuilderInterface
            - Symfony\Component\Form\FormInterface
            - Symfony\Component\Form\FormTypeExtensionInterface
            - Symfony\Component\Form\FormTypeInterface
{% endif %}
