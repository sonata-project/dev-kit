# DO NOT EDIT THIS FILE!
#
# It's auto-generated by sonata-project/dev-kit package.

includes:
{% if file_exists(project_dir ~ '/phpstan-baseline.neon') %}
    - phpstan-baseline.neon
{% endif %}
    - vendor/phpstan/phpstan/conf/bleedingEdge.neon

parameters:
    level: 8
{% if file_exists(project_dir ~ '/phpstan-console-application.php') %}
    symfony:
        console_application_loader: ./phpstan-console-application.php
{% endif %}
    paths:
        - src
        - tests
    scanFiles:
        - vendor/symfony/dependency-injection/Loader/Configurator/ContainerConfigurator.php
{% if file_exists(project_dir ~ '/src/Resources/skeleton') %}
    excludePaths:
        analyseAndScan:
            - src/Resources/skeleton/**
{% endif %}
    treatPhpDocTypesAsCertain: false
    checkGenericClassInNonGenericObjectType: true
    checkInternalClassCaseSensitivity: true
    checkMissingIterableValueType: true
    checkMissingVarTagTypehint: true
    checkMissingTypehints: true
    checkUninitializedProperties: true
